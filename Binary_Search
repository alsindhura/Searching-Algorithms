# defines a function named binary_search which consists of parameters array, starting_index, ending_index and search_element 
def binary_search(arr,starting_index,ending_index,search_element):
    #finding the mid in the array
    if starting_index<=ending_index:
        mid=(starting_index+ending_index)//2

        # if search_element is the mid
        if arr[mid]==search_element:
            return mid

        # if mid >the search_element we must go the first half of the array
        elif arr[mid]>search_element:
            return binary_search(arr, starting_index, mid-1, search_element)

        # if mid<search_element we must go to the next half of the array
        elif arr[mid]<search_element:
            return binary_search(arr, mid+1, ending_index, search_element)
    
    # if the element is not present in the array return -1
    else:
        return -1

#gets user input for the array
arr=[int(i) for i in input().split()]

#gets the user input for the search_element 
search_element=int(input())

#calling the function
result=binary_search(arr,0,len(arr)-1,search_element)

# printing the result
if result==-1:
    print("The element is not found in the list")
else:
    print("The element is found in the list at the index:", result)